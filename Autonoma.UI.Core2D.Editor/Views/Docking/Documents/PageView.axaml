<UserControl x:Class="Autonoma.UI.Operation.Views.Docking.Documents.PageView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:ce="clr-namespace:Core2D.Controls.Editor;assembly=Core2D"
             xmlns:ace="using:Autonoma.UI.Core2D.Editor.Controls.Editor"
             xmlns:vm="clr-namespace:Core2D.ViewModels.Docking.Documents;assembly=Core2D"
             mc:Ignorable="d"
             d:DesignWidth="800" d:DesignHeight="600"
             x:DataType="vm:PageViewModel" x:CompileBindings="True">
	<Grid ColumnDefinitions="Auto,*">
		<ace:ToolsView DataContext="{Binding Context}" Grid.Column="0" />
		<ce:PageView DataContext="{Binding Context}" Grid.Column="1" />
	</Grid>
</UserControl>
